// fibbonacci
Q.2
#include <stdio.h>
int main()
{
    int num;
    printf("Enter Any Number");
    scanf("%d",&num);
    int n1=0,n2=1,n3;
    for(int i=1;i<=num;i++){
        n3 = n1+n2;
        printf("%d ",n3);
        n1=n2;
        n2=n3;
    }

    return 0;
}
-------------------------
//pallindrome
Q.3
#include <stdio.h>
int main()
{
   int num;
   printf("Enter Any Number");
   scanf("%d",&num);
   int rem,sum=0;
   int temp=num;
   while(num>0){
       rem=num%10;
       sum=sum*10+rem;
       num=num/10;
   }
   if(temp==sum){
       printf("this number is pallindrome");
   }
   else {
       printf("this number is not pallindrome");
   }

    return 0;
}

