Q.10
#include <stdio.h>
int main()
{
   for(int i=1; i<=5; i++){
       for(int j=1; j<=5; j++){
           if(i==1 || j==1 || i==5){
               printf("*");
           }
           
       }
       printf("\n");
   }
    return 0;
}
----------------------------------------------
Q.11
#include <stdio.h>

int main()
{
   for(int i=1;i<=4;i++){
      for(int s=3;s>=i;s--){
          printf(" ");
      }
      for(int j=1;j<=i;j++){
          printf(" *");
      }
       printf("\n");
   }
    return 0;
}
