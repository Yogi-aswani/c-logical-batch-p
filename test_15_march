//count on digits
Q.4
#include <stdio.h>
int main()
{
    int num;
    printf("Enter Any Number");
    scanf("%d",&num);
    int count = 0;
    while(num>0){
        count++;
        num = num/10;
    }
    printf("%d\n",count);
   

    return 0;
}
