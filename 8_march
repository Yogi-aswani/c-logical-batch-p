Q.8 : User Input start to end value
#include <stdio.h>

int main()
{   int a;
    int b; 
    printf("Emter The Start Value");
    scanf("%d",&a);
     printf("Emter The End Value");
    scanf("%d",&b);
    if(a<b){
        while(a<=b){
            if(a%2==0){
                printf("%d ",a);
            }
            a++;
        }
    }
    else{
         while(a>=b){
            if(a%2==0){
                printf("%d ",a);
            }
            a--;
        }
    }
    return 0;
}
--------------------------------------------------------------
Q.9 : 1---->100 odd number print
#include <stdio.h>

int main()
{   int num = 1;
    while(num<=100){
        if(num%2!=0){
            printf("%d ",num);
           
        }
         num++;
    }

    return 0;
}
